{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"baseUrl": "./",
		"moduleResolution": "node",
		"paths": {
			"@/*": ["src/*"],
			"@/assets*": ["src/assets/*"],
			"@/components*": ["src/components/*"],
			"@/utils*": ["src/utils/*"]
		},
		"lib": ["esnext", "dom", "dom.iterable", "scripthost"]
	}
}
